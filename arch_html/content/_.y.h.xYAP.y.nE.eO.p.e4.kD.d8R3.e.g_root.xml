ty canonicalName="owner" name="Owner" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="package" name="Package" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="qualifiedName" name="Qualified Name" publish:category="UML" publish:value="nsls2_data::kitchen_sink::HXN_DEV: Tomography with self-absorption correction"/><publish:property canonicalName="visibility" name="Visibility" publish:category="UML" publish:value="Public"/></publish:properties><ownedComment annotatedElement="_Z.yM.d.sP.yYE.eO.p.e4.kD.d8R3.e.g" body="&lt;p&gt;&lt;span style=&quot;font-size:14pt;&quot;&gt;HXN&lt;/span&gt;&lt;/p&gt;&lt;p&gt;From Gabe Iltis's notes 6-25-14&lt;/p&gt;&lt;p&gt;For this task, we expect to do tomography based on different data, such as absorption, phase and fluorescence.&lt;/p&gt;&lt;p&gt;• An automatic alignment method needs to be developed for both vertical and horizontal directions.&lt;/p&gt;&lt;p&gt;• Self-absorption effect needs to be emphasized, and correction options need to be available.&lt;/p&gt;&lt;p&gt;• Current algorithms such as FBP, ART need to be considered.&lt;/p&gt;&lt;p&gt;This tool is being developed by Li Li and is 30% complete as of 6-25-14&lt;/p&gt;" isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="-1325061591_icon.png" publish:qualifiedname="nsls2_data::kitchen_sink::HXN_DEV: Tomography with self-absorption correction::&lt;Comment&gt;" xmi:id="_.f.k.cJIP.yYE.eO.p.e4.kD.d8R3.e.g" xsi:type="uml:Comment"><publish:properties><publish:property canonicalName="body" name="Body" publish:category="UML" publish:value="&lt;p&gt;&lt;span style=&quot;font-size:14pt;&quot;&gt;HXN&lt;/span&gt;&lt;/p&gt;&lt;p&gt;From Gabe Iltis's notes 6-25-14&lt;/p&gt;&lt;p&gt;For this task, we expect to do tomography based on different data, such as absorption, phase and fluorescence.&lt;/p&gt;&lt;p&gt;• An automatic alignment method needs to be developed for both vertical and horizontal directions.&lt;/p&gt;&lt;p&gt;• Self-absorption effect needs to be emphasized, and correction options need to be available.&lt;/p&gt;&lt;p&gt;• Current algorithms such as FBP, ART need to be considered.&lt;/p&gt;&lt;p&gt;This tool is being developed by Li Li and is 30% complete as of 6-25-14&lt;/p&gt;"/><publish:property canonicalName="owner" name="Owner" publish:category="UML" publish:ismodellink="false" publish:value="HXN_DEV: Tomography with self-absorption correction" xmi:id="_Z.yM.d.sP.yYE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="Stereotype" name="Stereotype" publish:category="UML" publish:value="documentation"/></publish:properties></ownedComment></packagedElement><packagedElement name="HXN_DEV: XRF Microscopy" publish:icon="-234542445_icon.png" publish:qualifiedname="nsls2_data::kitchen_sink::HXN_DEV: XRF Microscopy" xmi:id="_-2PN.kP.yXE.eO.p.e4.kD.d8R3.e.g" xsi:type="uml:UseCase"><publish:properties><publish:property canonicalName="isAbstract" name="Is Abstract" publish:category="UML" publish:value="false"/><publish:property canonicalName="isLeaf" name="Is Leaf" publish:category="UML" publish:value="false"/><publish:property canonicalName="name" name="Name" publish:category="UML" publish:value="HXN_DEV: XRF Microscopy"/><publish:property canonicalName="namespace" name="Namespace" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="owner" name="Owner" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="package" name="Package" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="qualifiedName" name="Qualified Name" publish:category="UML" publish:value="nsls2_data::kitchen_sink::HXN_DEV: XRF Microscopy"/><publish:property canonicalName="visibility" name="Visibility" publish:category="UML" publish:value="Public"/></publish:properties><ownedComment annotatedElement="_-2PN.kP.yXE.eO.p.e4.kD.d8R3.e.g" body="&lt;p&gt;&lt;span style=&quot;font-size:14pt;&quot;&gt;HXN&lt;/span&gt;&lt;/p&gt;&lt;p&gt;From Gabe's notes: 6-25-14&lt;/p&gt;&lt;p&gt;This analysis tool mainly performs the following tasks:&lt;/p&gt;&lt;ol&gt;&lt;li&gt;&lt;p&gt;Sum spectrum peak fitting: sum individual spectrum from each pixel and do fitting to determine which elements exist in the sample.&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Single pixel peak fitting to quantify the element in each 2D pixel&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Visualization:&lt;/p&gt;&lt;/li&gt;&lt;ol&gt;&lt;li&gt;&lt;p&gt;Overall spectrum with fitting&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Spectrum from individual pixel&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Spectrum over spatial ROI&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;ROI element maps&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;ROI fitted element maps&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Error maps, i.e. chi^2&lt;/p&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/ol&gt;&lt;p&gt;This analysis tool is being developed by Li Li and is 90% complete, as of 6-25-14&lt;/p&gt;" isDocumentation="true" isURL="false" name="&lt;Unnamed Comment&gt;" publish:icon="-1325061591_icon.png" publish:qualifiedname="nsls2_data::kitchen_sink::HXN_DEV: XRF Microscopy::&lt;Comment&gt;" xmi:id="_O.tP.i.gP.yYE.eO.p.e4.kD.d8R3.e.g" xsi:type="uml:Comment"><publish:properties><publish:property canonicalName="body" name="Body" publish:category="UML" publish:value="&lt;p&gt;&lt;span style=&quot;font-size:14pt;&quot;&gt;HXN&lt;/span&gt;&lt;/p&gt;&lt;p&gt;From Gabe's notes: 6-25-14&lt;/p&gt;&lt;p&gt;This analysis tool mainly performs the following tasks:&lt;/p&gt;&lt;ol&gt;&lt;li&gt;&lt;p&gt;Sum spectrum peak fitting: sum individual spectrum from each pixel and do fitting to determine which elements exist in the sample.&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Single pixel peak fitting to quantify the element in each 2D pixel&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Visualization:&lt;/p&gt;&lt;/li&gt;&lt;ol&gt;&lt;li&gt;&lt;p&gt;Overall spectrum with fitting&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Spectrum from individual pixel&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Spectrum over spatial ROI&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;ROI element maps&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;ROI fitted element maps&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p&gt;Error maps, i.e. chi^2&lt;/p&gt;&lt;/li&gt;&lt;/ol&gt;&lt;/ol&gt;&lt;p&gt;This analysis tool is being developed by Li Li and is 90% complete, as of 6-25-14&lt;/p&gt;"/><publish:property canonicalName="owner" name="Owner" publish:category="UML" publish:ismodellink="false" publish:value="HXN_DEV: XRF Microscopy" xmi:id="_-2PN.kP.yXE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="Stereotype" name="Stereotype" publish:category="UML" publish:value="documentation"/></publish:properties></ownedComment></packagedElement><packagedElement name="CHX_DEV: 1-time correlation" publish:icon="-234542445_icon.png" publish:qualifiedname="nsls2_data::kitchen_sink::CHX_DEV: 1-time correlation" xmi:id="_2.jX.lMP.y.nE.eO.p.e4.kD.d8R3.e.g" xsi:type="uml:UseCase"><publish:properties><publish:property canonicalName="isAbstract" name="Is Abstract" publish:category="UML" publish:value="false"/><publish:property canonicalName="isLeaf" name="Is Leaf" publish:category="UML" publish:value="false"/><publish:property canonicalName="name" name="Name" publish:category="UML" publish:value="CHX_DEV: 1-time correlation"/><publish:property canonicalName="namespace" name="Namespace" publish:category="UML" publish:ismodellink="false" publish:value="kitchen_sink" xmi:id="_.y.h.xYAP.y.nE.eO.p.e4.kD.d8R3.e.g"/><publish:property canonicalName="owner" name="Owner" 